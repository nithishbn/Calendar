            # size: 3 * dp(48), dp(48)
            opposite_colors: True
            pos_hint: {'center_y': 1,'right': 1}
            on_release:
                root.manager.transition = NoTransition()
                root.register()
    BoxLayout:
        orientation: 'vertical'
        size_hint: 0.88, 0.15
        pos_hint: {'center_x': 0.36, 'center_y': 0.20}
        # canvas.before:
